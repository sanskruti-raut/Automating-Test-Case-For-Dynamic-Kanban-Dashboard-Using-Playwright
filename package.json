{
  "name": "kanban-automation-test",
  "version": "1.0.0",
  "description": "Playwright automation tests for Kanban application",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:repeat": "npx playwright test --repeat-each 5",
    "test:repeat-headed": "npx playwright test --repeat-each 5 --headed",
    "test:debug": "npx playwright test --debug",
    "report": "npx playwright show-report",
    "install:browsers": "npx playwright install"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "kanban"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.56.0"
  },
  "directories": {
    "test": "tests"
  }
}
